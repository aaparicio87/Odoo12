#: model:mail.template,report_name:sales_modification.sale_incentive_email_template
msgid "${(object.name or '').replace('/','_')}"
msgstr ""

#. module: sales_modification
#: model:mail.template,subject:sales_modification.email_template_quote_sale
msgid "${object.company_id.name} ${object.state in ('draft', 'sent') and 'Quotation' or 'Order'} (Ref ${object.name or 'n/a' })"
msgstr "${object.company_id.name} ${object.state in ('draft', 'sent') and 'Cotización' or 'Orden'} (Ref ${object.name or 'n/a' })"

#. module: sales_modification
#: model:mail.template,body_html:sales_modification.email_template_quote_sale
msgid "<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                    % set doc_name = 'quotation' if object.state in ('draft', 'sent') else 'order'\n"
"                    Dear ${object.partner_id.name}\n"
"                    % if object.partner_id.parent_id:\n"
"                        (${object.partner_id.parent_id.name})\n"
"                    % endif\n"
"                    <br/><br/>\n"
"                    Here is\n"
"                    % if ctx.get('proforma')\n"
"                        in attachment your pro-forma invoice\n"
"                    % else\n"
"                        the ${doc_name} <strong>${object.name}</strong>\n"
"                    % endif\n"
"                    % if object.origin:\n"
"                        (with reference: ${object.origin} )\n"
"                    % endif\n"
"                    amounting in <strong>${format_amount(object.amount_total, object.pricelist_id.currency_id)}</strong>\n"
"                    from ${object.company_id.name}.\n"
"                    <br/><br/>\n"
"                    Do not hesitate to contact us if you have any question.\n"
"                </p>\n"
"            </div>\n"
"            "
msgstr "<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                    % set doc_name = 'cotización' if object.state in ('draft', 'sent') else 'orden'\n"
"                    Estimado ${object.partner_id.name}\n"
"                    % if object.partner_id.parent_id:\n"
"                        (${object.partner_id.parent_id.name})\n"
"                    % endif\n"
"                    <br/><br/>\n"
"                    Aquí\n"
"                    % if ctx.get('proforma')\n"
"                        se adjunta su pro-forma de factura\n"
"                    % else\n"
"                        el ${doc_name} <strong>${object.name}</strong>\n"
"                    % endif\n"
"                    % if object.origin:\n"
"                        (con referencia: ${object.origin} )\n"
"                    % endif\n"
"                    ascendiendo en <strong>${format_amount(object.amount_total, object.pricelist_id.currency_id)}</strong>\n"
"                    de ${object.company_id.name}.\n"
"                    <br/><br/>\n"
"                    No dude en contactar con nosotros si tienes alguna duda.\n"
"                </p>\n"
"            </div>\n"
"            "

